  <Identity Name="BingtangXH.Anemo-TileWizard" Version="1.1.3.31" Publisher="CN=ninei" ProcessorArchitecture="neutral" />
  <Properties>
    <DisplayName>Anemo-TileWizard.Windows</DisplayName>
    <PublisherDisplayName>BingtangXH</PublisherDisplayName>
    <Logo>images\storelogo.png</Logo>
  </Properties>
  <Prerequisites>
    <OSMinVersion>6.3.0</OSMinVersion>
    <OSMaxVersionTested>6.3.0</OSMaxVersionTested>
  </Prerequisites>
  <Resources>
    <Resource Language="ZH-CN" />
  </Resources>
  <Applications>
    <Application Id="App" StartPage="default.html">
      <m2:VisualElements DisplayName="Anemo TileWizard" Description="Anemo-TileWizard.Windows" ForegroundText="light" BackgroundColor="#02787D" Square150x150Logo="images\Logo.png" Square30x30Logo="images\SmallLogo.png" ToastCapable="true">
